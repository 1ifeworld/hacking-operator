#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/salieflewis/Desktop/river-project/river-net/node_modules/.pnpm/tsx@3.13.0/node_modules/tsx/dist/node_modules:/Users/salieflewis/Desktop/river-project/river-net/node_modules/.pnpm/tsx@3.13.0/node_modules/tsx/node_modules:/Users/salieflewis/Desktop/river-project/river-net/node_modules/.pnpm/tsx@3.13.0/node_modules:/Users/salieflewis/Desktop/river-project/river-net/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/salieflewis/Desktop/river-project/river-net/node_modules/.pnpm/tsx@3.13.0/node_modules/tsx/dist/node_modules:/Users/salieflewis/Desktop/river-project/river-net/node_modules/.pnpm/tsx@3.13.0/node_modules/tsx/node_modules:/Users/salieflewis/Desktop/river-project/river-net/node_modules/.pnpm/tsx@3.13.0/node_modules:/Users/salieflewis/Desktop/river-project/river-net/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsx/dist/cli.mjs" "$@"
else
  exec node  "$basedir/../tsx/dist/cli.mjs" "$@"
fi
