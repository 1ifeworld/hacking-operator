{
    "compilerOptions": {
      "target": "ESNext",
      "module": "CommonJS",
      "strict": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "esModuleInterop": true,
      "noEmit": true,
      "noImplicitReturns": true,
      "skipLibCheck": true,
      "baseUrl": ".",
      "paths": {
        "@/abi": ["abi/index.ts"],
        "@/constants": ["constants/index.ts"],
        "@/helpers": ["helpers/index.ts"],
        "@/pimlicoConfig": ["pimlicoConfig/index.ts"],
        "@/services": ["services/index.ts"],
        "@/utils": ["utils/index.ts"],
        "@/viemConfig": ["viemConfig/index.ts"],
        "@/actions": ["src/provider/index.ts"],
      }
    },
    "exclude": ["node_modules"],
  }
  